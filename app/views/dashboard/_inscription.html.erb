<div class="Sign_Form">
	<h1>Sign Up</h1>
	<%= form_for(:member, :url => {:controller => 'members', :action => 'create'}) do |f| %>
		<p> Username:</br> <%= f.text_field :firstname%> </p>
		<p> Email:</br> <%= f.text_field :email%> </p>
		<p> Password:</br> <%= f.password_field :password%></p>
		<p> Password Confirmation:</br> <%= f.password_field :password_confirmation%> </p>
		<%= f.submit :Signup %>
	<% end %>
</div>
