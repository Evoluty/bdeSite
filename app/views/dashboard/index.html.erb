
<div class="row" id="dashMain">
	<div class="col">
		<div id="dashTab">
		  <ul class="tabs">
		    <li class="tab"><a href="#test1" class="active col">News</a></li>
		    <li class="tab"><a href="#test2" class="col">Clubs</a></li>
		    <li class="tab"><a href="#test3" class="col">Partenaires</a></li>
		    <li class="tab"><a href="#test4" class="col">Bde</a></li>
		    <li class="tab"><a href="#inscription" class="col">Inscription</a></li>
		  </ul>
		</div>
	</div>
	<div class="col s8 offset-s1">
		<% flash.each do |name, msg| -%>
			<script type="text/javascript">
				$(document).ready(function () {
					(function($){
						$(function(){
					 		Materialize.toast('<%= msg %>', 3000, 'rounded <%= name %>');
						}); // end of docum	ent ready
					})(jQuery); // end of jQuery name space
				});
			</script>
		<% end -%>

		<div id="test1" class="col s12">
			<%= render "dashboard/news" %>
		</div>

		<div id="test2" class="col s12">
			<%= render "dashboard/clubs" %>
		</div>

		<div id="test3" class="col s12">
			<%= render "dashboard/partenaires" %>
		</div>

		<div id="test4" class="col s12">
			<%= render "dashboard/bde" %>
		</div>

		<div id="inscription" class="col s12">
			<%= render "dashboard/inscription" %>
		</div>
		
	</div>
</div>
